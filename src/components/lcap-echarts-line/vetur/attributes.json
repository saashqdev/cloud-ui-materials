{
  "lcap-echarts-line/data-source": {
    "type": "Array<Item> | Function | object | DataSource",
    "description": "表格的数据源，数据集对象或者返回数据集的逻辑"
  },
  "lcap-echarts-line/data-schema": {
    "type": "schema",
    "description": "表格每一行的数据类型"
  },
  "lcap-echarts-line/undefinedToZero": {
    "type": "string",
    "options": [
      {
        "value": "empty",
        "title": "空距"
      },
      {
        "value": "zero",
        "title": "零"
      }
    ],
    "description": "设置将未定义的值如undefined，null转换为0或空距"
  },
  "lcap-echarts-line/initialLoad": {
    "type": "boolean",
    "description": "是否在初始时立即加载"
  },
  "lcap-echarts-line/xAxis": {
    "type": "string",
    "description": "设置维度(统计类别)"
  },
  "lcap-echarts-line/yAxis": {
    "type": "string",
    "description": "设置度量（统计值）"
  },
  "lcap-echarts-line/title": {
    "type": "string",
    "description": "设置图表标题"
  },
  "lcap-echarts-line/axisSplitLine": {
    "type": "string",
    "options": [
      {
        "title": "无",
        "value": "hidden"
      },
      {
        "title": "水平网格线",
        "value": "horizontal"
      },
      {
        "title": "垂直网格线",
        "value": "vertical"
      },
      {
        "title": "全部网格线",
        "value": "both"
      }
    ],
    "description": "设置坐标系网格线"
  },
  "lcap-echarts-line/axisSplitLineType": {
    "type": "string",
    "options": [
      {
        "title": "实线",
        "value": "solid"
      },
      {
        "title": "虚线",
        "value": "dotted"
      },
      {
        "title": "点线",
        "value": "dashed"
      }
    ],
    "description": "设置网格线线型"
  },
  "lcap-echarts-line/theme": {
    "type": "string",
    "options": [
      {
        "value": "theme1",
        "title": "科技"
      },
      {
        "value": "theme2",
        "title": "清新"
      },
      {
        "value": "theme3",
        "title": "淡雅"
      },
      {
        "value": "theme4",
        "title": "经典"
      }
    ],
    "description": "设置图表配色方案"
  },
  "lcap-echarts-line/labelPosition": {
    "type": "string",
    "options": [
      {
        "value": "top",
        "title": "上"
      },
      {
        "value": "bottom",
        "title": "下"
      },
      {
        "value": "left",
        "title": "左"
      },
      {
        "value": "right",
        "title": "右"
      },
      {
        "value": "inside",
        "title": "居中"
      },
      {
        "value": "hidden",
        "title": "无"
      }
    ],
    "description": "设置数据标签位置"
  },
  "lcap-echarts-line/allowShowHint": {
    "type": "boolean",
    "description": "设置是否显示提示"
  },
  "lcap-echarts-line/allowShowLegend": {
    "type": "boolean",
    "description": "设置是否显示图例"
  },
  "lcap-echarts-line/legendName": {
    "type": "string",
    "description": "设置图例别名；修改成功后，图例名字会从\"指标\"改为\"别名\""
  },
  "lcap-echarts-line/areaFilled": {
    "type": "boolean",
    "description": "设置是否填充折线下方区域"
  },
  "lcap-echarts-line/lineType": {
    "type": "string",
    "options": [
      {
        "value": "solid",
        "title": "实线"
      },
      {
        "value": "dotted",
        "title": "虚线"
      },
      {
        "value": "dashed",
        "title": "点线"
      }
    ],
    "description": "设置线条类型"
  },
  "lcap-echarts-line/lineStyleSmooth": {
    "type": "string",
    "options": [
      {
        "value": "normal",
        "title": "基础"
      },
      {
        "value": "smooth",
        "title": "平滑"
      }
    ],
    "description": "设置线条样式"
  },
  "lcap-echarts-line/lineStyleSymbol": {
    "type": "string",
    "options": [
      {
        "value": "emptyCircle",
        "title": "空心圆"
      },
      {
        "value": "circle",
        "title": "圆形"
      },
      {
        "value": "rect",
        "title": "矩形"
      },
      {
        "value": "triangle",
        "title": "三角形"
      },
      {
        "value": "diamond",
        "title": "菱形"
      },
      {
        "value": "pin",
        "title": "水滴"
      },
      {
        "value": "arrow",
        "title": "箭头"
      },
      {
        "value": "none",
        "title": "无"
      }
    ]
  },
  "lcap-echarts-line/xAxisTitle": {
    "type": "string",
    "description": "设置x轴标题"
  },
  "lcap-echarts-line/showXAxisLine": {
    "type": "boolean",
    "description": "设置显示X轴轴线"
  },
  "lcap-echarts-line/showXAxisLabel": {
    "type": "boolean",
    "description": "设置是否显示X轴文字标签"
  },
  "lcap-echarts-line/xAxisLabelRotate": {
    "type": "string",
    "options": [
      {
        "value": "0",
        "title": "横向"
      },
      {
        "value": "90",
        "title": "纵向"
      },
      {
        "value": "45",
        "title": "左倾斜"
      },
      {
        "value": "-45",
        "title": "右倾斜"
      }
    ],
    "description": "设置显示X轴文字标签的方向"
  },
  "lcap-echarts-line/yAxisTitle": {
    "type": "string",
    "description": "设置Y轴标题"
  },
  "lcap-echarts-line/showYAxisLine": {
    "type": "boolean",
    "description": "设置显示Y轴轴线"
  },
  "lcap-echarts-line/showYAxisLabel": {
    "type": "boolean",
    "description": "设置是否显示Y轴数值标签"
  },
  "lcap-echarts-line/titleFontStyle": {
    "type": "string",
    "options": [
      {
        "value": "normal",
        "title": "正常"
      },
      {
        "value": "italic",
        "title": "斜体"
      }
    ],
    "description": "设置普通文字字号"
  },
  "lcap-echarts-line/allowDownload": {
    "type": "boolean",
    "description": "允许用户设置保存图片"
  }
}