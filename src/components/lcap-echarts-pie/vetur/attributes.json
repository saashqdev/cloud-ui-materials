{
  "lcap-echarts-pie/data-source": {
    "type": "Array<Item> | Function | object | DataSource",
    "description": "The data source of the table, the dataset object or the logic of returning the dataset"
  },
  "lcap-echarts-pie/data-schema": {
    "type": "schema",
    "description": "The data type of each row in the table"
  },
  "lcap-echarts-pie/undefinedToZero": {
    "type": "string",
    "options": [
      {
        "value": "empty",
        "title": "Space"
      },
      {
        "value": "zero",
        "title": "Zero"
      }
    ],
    "description": "Set the conversion of undefined values   such as undefined and null to 0 or empty space"
  },
  "lcap-echarts-pie/initialLoad": {
    "type": "boolean",
    "description": "Whether to load immediately at the initialization"
  },
  "lcap-echarts-pie/xAxis": {
    "type": "string",
    "description": "Set dimension (statistical category)"
  },
  "lcap-echarts-pie/yAxis": {
    "type": "string",
    "description": "Set metrics (statistics)"
  },
  "lcap-echarts-pie/title": {
    "type": "string",
    "description": "Set the main title"
  },
  "lcap-echarts-pie/theme": {
    "type": "string",
    "options": [
      {
        "value": "theme1",
        "title": "Technology"
      },
      {
        "value": "theme2",
        "title": "Fresh"
      },
      {
        "value": "theme3",
        "title": "Elegance"
      },
      {
        "value": "theme4",
        "title": "Classic"
      }
    ],
    "description": "Set the chart color scheme"
  },
  "lcap-echarts-pie/showLabelName": {
    "type": "boolean",
    "description": "Set whether to display dimension labels"
  },
  "lcap-echarts-pie/showLabelValue": {
    "type": "boolean",
    "description": "Set whether to display the value label"
  },
  "lcap-echarts-pie/showLabelPercent": {
    "type": "boolean",
    "description": "Set whether to display the percentage label"
  },
  "lcap-echarts-pie/allowShowHint": {
    "type": "boolean",
    "description": "Set whether to display prompts"
  },
  "lcap-echarts-pie/allowShowLegend": {
    "type": "boolean",
    "description": "Set whether to display the legend"
  },
  "lcap-echarts-pie/legendPosition": {
    "type": "string",
    "options": [
      {
        "value": "top",
        "title": "Top"
      },
      {
        "value": "bottom",
        "title": "Bottom"
      },
      {
        "value": "left",
        "title": "Left"
      },
      {
        "value": "right",
        "title": "Right"
      }
    ],
    "description": "Set legend position"
  },
  "lcap-echarts-pie/legendScroll": {
    "type": "string",
    "options": [
      {
        "value": "normal",
        "title": "Normal"
      },
      {
        "value": "scroll",
        "title": "Scroll"
      }
    ],
    "description": "Set whether the legend should scroll"
  },
  "lcap-echarts-pie/pieType": {
    "type": "string",
    "options": [
      {
        "value": "pie",
        "title": "Pie Type"
      },
      {
        "value": "circle",
        "title": "Ring Type"
      },
      {
        "value": "semiCircle",
        "title": "Semi-Circular"
      }
    ]
  },
  "lcap-echarts-pie/titleFontStyle": {
    "type": "string",
    "options": [
      {
        "value": "normal",
        "title": "Normal"
      },
      {
        "value": "italic",
        "title": "Italic"
      }
    ],
    "description": "Set normal text font size"
  },
  "lcap-echarts-pie/allowDownload": {
    "type": "boolean",
    "description": "Allows users to save pictures"
  }
}