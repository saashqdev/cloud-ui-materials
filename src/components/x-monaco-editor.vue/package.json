  },
  "peerDependencies": {
    "@vue/cli-service": ">=3.10.0",
    "vue-cli-plugin-kubevue": "^0.11.11",
    "vue-cli-plugin-monaco": "^0.1.1",
    "vue": "^2.6.10",
    "cloud-ui.kubevue": "^0.9.13"
  },
  "vetur": {
